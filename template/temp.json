{
  "name": "flutter-commands",
  "version": "1.0.0",
  "description": "flutter 常用命令",
  "scripts": {
    "install": "flutter pub get",
    "i": "flutter pub get",
    "test": "flutter test",
    "dev": "flutter run --dart-define=ENV=dev",
    "build:android:vsit": "flutter build apk lib/main_vsit.dart",
    "build:android:canary": "flutter build apk lib/main_canary.dart",
    "build:android": "flutter build apk lib/main.dart",
    "build:android:production": "flutter build apk lib/main.dart",
    "palette": "ts-node ./lib/style/generate-palette.ts",
    "commit": "cz",
    "pull": "git pull --autostash",
    "lint": "flutter analyze --no-fatal-infos --no-fatal-warnings"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/node": "^18.11.9",
    "commitizen": "^4.2.5",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
